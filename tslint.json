{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [100]
    },
    "quotemark": [true, "single"],
    "curly": [true, "ignore-same-line"],
    "no-bitwise": false,
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": false,
    "interface-name": false,
    "max-classes-per-file": false,
    "member-access": true
  }
}
